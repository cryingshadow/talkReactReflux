{
  "name": "webshop-example",
  "version": "1.1.6",
  "description": "A stripped down webshop example to showcase react and reflux.",
  "main": "webpack.config.js",
  "devDependencies": {
    "babel": "~6.23.0",
    "babel-cli": "~6.26.0",
    "babel-core": "~6.26.0",
    "babel-loader": "~6.4.1",
    "babel-preset-es2015": "~6.22.0",
    "babel-preset-react": "^6.22.0",
    "html-webpack-plugin": "^2.21.0",
    "css-loader": "0.28.4",
    "style-loader": "0.18.2",
    "file-loader": "0.11.2",
    "http-server": "^0.9.0",
    "webpack": "~1.13.1",
    "webpack-dev-server": "~1.14.1",
    "webpack-merge": "~0.14.0",
    "sass-loader": "^6.0.6",
    "node-sass": "^4.5.2",
    "react-dom": "^15.6.1",
    "reactstrap": "^4.8.0",
    "react-addons-css-transition-group": "^15.6.0",
    "react-addons-transition-group": "^15.6.0",
    "bootstrap": "^4.0.0-alpha.4",
    "jest": "^21.1.0",
    "babel-jest": "^21.0.2",
    "react-test-renderer": "^15.6.1",
    "enzyme": "^2.9.1",
    "jest-enzyme": "^3.8.3"
  },
  "jest": {
    "moduleFileExtensions": ["js", "jsx"],
    "setupTestFrameworkScriptFile": "./node_modules/jest-enzyme/lib/index.js",
    "unmockedModulePathPatterns": [
      "react",
      "enzyme",
      "jest-enzyme"
    ],
    "moduleDirectories": [
      "src",
      "node_modules",
      "build"
    ],
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/__mocks__/fileMock.js",
      "^.+\\.(css|scss|less)$": "<rootDir>/src/__mocks__/styleMock.js"
    }
  },
  "scripts": {
    "build": "webpack",
    "test": "jest",
    "start": "npm i && webpack && node ./node_modules/http-server/bin/http-server ./build -p 8080 -o"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thomasstroeder/talkReactReflux"
  },
  "author": "Philip Baues, Thomas Str√∂der",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/thomasstroeder/talkReactReflux/issues"
  },
  "dependencies": {
    "react": "15.6.0",
    "react-dom": "15.6.0",
    "reflux": "^6.4.1"
  }
}
